{
	"useTabs": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"arrowParens": "avoid",
	"bracketSameLine": false,
	"endOfLine": "lf",
	"vueIndentScriptAndStyle": false,
	"trailingComma": "all",
	"semi": false,
	"singleQuote": true,
	"singleAttributePerLine": true
}
